  "phrases": [
    {
      "english_term": "How are you?",
      "native_term": "സുഖമാണോ?",
      "transliteration": "Sukhamaano?"
    },
    {
      "english_term": "I am fine.",
      "native_term": "എനിക്ക് സുഖമാണ്.",
      "transliteration": "Enikku sukhamanu."
    },
    {
      "english_term": "What is your name?",
      "native_term": "പേര് എന്താണ്?",
      "transliteration": "Peru enthaanu?"
    },
    {
      "english_term": "My name is...",
      "native_term": "എൻ്റെ പേര്... ആണ്.",
      "transliteration": "Ente peru... aanu."
    },
    {
      "english_term": "How much is this?",
      "native_term": "ഇതിന് എത്രയാണ്?",
      "transliteration": "Ithinu ethrayaanu?"
    },
    {
      "english_term": "Where is the bathroom?",
      "native_term": "Bathroom എവിടെയാണ്?",
      "transliteration": "Bathroom evideyaanu?"
    },
    {
      "english_term": "I don't understand.",
      "native_term": "എനിക്ക് മനസ്സിലായില്ല.",
      "transliteration": "Enikku manasilayilla."
    },
    {
      "english_term": "Do you speak English?",
      "native_term": "English സംസാരിക്കുമോ?",
      "transliteration": "English samsarikkumo?"
    },
    {
      "english_term": "Please help me.",
      "native_term": "എന്നെ സഹായിക്കൂ.",
      "transliteration": "Enne sahaayikku."
    },
    {
      "english_term": "Excuse me / Listen.",
      "native_term": "ഒന്ന് ശ്രദ്ധിക്കൂ.",
      "transliteration": "Onnu shraddhikkoo."
    },
    {
      "english_term": "Stop here.",
      "native_term": "ഇവിടെ നിർത്തൂ.",
      "transliteration": "Ivide nirthoo."
    },
    {
      "english_term": "I want water.",
      "native_term": "എനിക്ക് വെള്ളം വേണം.",
      "transliteration": "Enikku vellam venam."
    },
    {
      "english_term": "I am hungry.",
      "native_term": "എനിക്ക് വിശക്കുന്നു.",
      "transliteration": "Enikku vishakkunnu."
    },
    {
      "english_term": "It is very good.",
      "native_term": "നല്ല രുചിയുണ്ട്.",
      "transliteration": "Nalla ruchiyundu."
    },
    {
      "english_term": "I need a taxi/auto.",
      "native_term": "എനിക്കൊരു Auto വേണം.",
      "transliteration": "Enikku oru Auto venam."
    },
    {
      "english_term": "See you later.",
      "native_term": "പിന്നെ കാണാം.",
      "transliteration": "Pinne kaanaam."
    },
    {
      "english_term": "What time is it?",
      "native_term": "സമയം എത്രയായി?",
      "transliteration": "Samayam ethrayayi?"
    },
    {
      "english_term": "Let's go.",
      "native_term": "നമുക്ക് പോകാം.",
      "transliteration": "Namukku pokaam."
    },
    {
      "english_term": "Wait a minute.",
      "native_term": "ഒരു മിനിറ്റ് നിൽക്കൂ.",
      "transliteration": "Oru minute nilkku."
    },
    {
      "english_term": "Sorry.",
      "native_term": "ക്ഷമിക്കണം.",
      "transliteration": "Kshamikkanam."
    }
  ]
