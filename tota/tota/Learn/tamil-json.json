  "phrases": [
    {
      "english_term": "How are you?",
      "native_term": "நீங்கள் எப்படி இருக்கிறீர்கள்?",
      "transliteration": "Neengal eppadi irukirargal?"
    },
    {
      "english_term": "I am fine.",
      "native_term": "நான் நன்றாக இருக்கிறேன்.",
      "transliteration": "Naan nandragha irukiren."
    },
    {
      "english_term": "What is your name?",
      "native_term": "உங்கள் பெயர் என்ன?",
      "transliteration": "Ungal peyar enna?"
    },
    {
      "english_term": "My name is...",
      "native_term": "என் பெயர்...",
      "transliteration": "En peyar..."
    },
    {
      "english_term": "How much is this?",
      "native_term": "இது எவ்வளவு?",
      "transliteration": "Idhu evvalavu?"
    },
    {
      "english_term": "Where is the bathroom?",
      "native_term": "Bathroom எங்கே இருக்கிறது?",
      "transliteration": "Bathroom enge irukiradhu?"
    },
    {
      "english_term": "I don't understand.",
      "native_term": "எனக்கு புரியவில்லை.",
      "transliteration": "Enakku puriyavillai."
    },
    {
      "english_term": "Do you speak English?",
      "native_term": "நீங்கள் English பேசுவீர்களா?",
      "transliteration": "Neengal English pesuveergala?"
    },
    {
      "english_term": "Please help me.",
      "native_term": "தயவுசெய்து உதவுங்கள்.",
      "transliteration": "Dayavuseidhu udhavungal."
    },
    {
      "english_term": "Excuse me / Listen.",
      "native_term": "மன்னிக்கவும்.",
      "transliteration": "Mannikkavum."
    },
    {
      "english_term": "Stop here.",
      "native_term": "இங்கே நிறுத்துங்கள்.",
      "transliteration": "Inge niruthungal."
    },
    {
      "english_term": "I want water.",
      "native_term": "எனக்கு தண்ணீர் வேண்டும்.",
      "transliteration": "Enakku thanneer vendum."
    },
    {
      "english_term": "I am hungry.",
      "native_term": "எனக்கு பசிக்கிறது.",
      "transliteration": "Enakku pasikkiradhu."
    },
    {
      "english_term": "It is very good.",
      "native_term": "இது மிகவும் நன்றாக உள்ளது.",
      "transliteration": "Idhu migavum nandragha ulladhu."
    },
    {
      "english_term": "I need a taxi/auto.",
      "native_term": "எனக்கு Auto வேண்டும்.",
      "transliteration": "Enakku Auto vendum."
    },
    {
      "english_term": "See you later.",
      "native_term": "பிறகு சந்திப்போம்.",
      "transliteration": "Piragu sandhippom."
    },
    {
      "english_term": "What time is it?",
      "native_term": "மணி என்ன?",
      "transliteration": "Mani enna?"
    },
    {
      "english_term": "Let's go.",
      "native_term": "போகலாம்.",
      "transliteration": "Pogalam."
    },
    {
      "english_term": "Wait a minute.",
      "native_term": "ஒரு நிமிடம் இருங்கள்.",
      "transliteration": "Oru nimidam irungal."
    },
    {
      "english_term": "Sorry.",
      "native_term": "மன்னிக்கவும்.",
      "transliteration": "Mannikkavum."
    }
  ]
