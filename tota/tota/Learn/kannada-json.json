  "phrases": [
    {
      "english_term": "How are you?",
      "native_term": "ನೀವು ಹೇಗಿದ್ದೀರಾ?",
      "transliteration": "Neevu hegiddira?"
    },
    {
      "english_term": "I am fine.",
      "native_term": "ನಾನು ಚೆನ್ನಾಗಿದ್ದೀನಿ.",
      "transliteration": "Naanu chennagiddini."
    },
    {
      "english_term": "What is your name?",
      "native_term": "ನಿಮ್ಮ ಹೆಸರೇನು?",
      "transliteration": "Nimma hesarenu?"
    },
    {
      "english_term": "My name is...",
      "native_term": "ನನ್ನ ಹೆಸರು...",
      "transliteration": "Nanna hesaru..."
    },
    {
      "english_term": "How much is this?",
      "native_term": "ಇದಕ್ಕೆ ಎಷ್ಟು?",
      "transliteration": "Idakke eshtu?"
    },
    {
      "english_term": "Where is the bathroom?",
      "native_term": "Bathroom ಎಲ್ಲಿದೆ?",
      "transliteration": "Bathroom ellide?"
    },
    {
      "english_term": "I don't understand.",
      "native_term": "ನನಗೆ ಅರ್ಥವಾಗಲಿಲ್ಲ.",
      "transliteration": "Nanage arthavagalilla."
    },
    {
      "english_term": "Do you speak English?",
      "native_term": "ನೀವು English ಮಾತನಾಡುತ್ತೀರಾ?",
      "transliteration": "Neevu English matanaduttira?"
    },
    {
      "english_term": "Please help me.",
      "native_term": "ದಯವಿಟ್ಟು ನನಗೆ ಸಹಾಯ ಮಾಡಿ.",
      "transliteration": "Dayavittu nanage sahaya madi."
    },
    {
      "english_term": "Excuse me / Listen.",
      "native_term": "ಕ್ಷಮಿಸಿ / ಇಲ್ಲಿ ಕೇಳಿ.",
      "transliteration": "Kshamisi / Illi keli."
    },
    {
      "english_term": "Stop here.",
      "native_term": "ಇಲ್ಲಿ ನಿಲ್ಲಿಸಿ.",
      "transliteration": "Illi nillisi."
    },
    {
      "english_term": "I want water.",
      "native_term": "ನನಗೆ ನೀರು ಬೇಕು.",
      "transliteration": "Nanage neeru beku."
    },
    {
      "english_term": "I am hungry.",
      "native_term": "ನನಗೆ ಹಸಿವಾಗಿದೆ.",
      "transliteration": "Nanage hasivagide."
    },
    {
      "english_term": "It is very good.",
      "native_term": "ಇದು ತುಂಬಾ ಚೆನ್ನಾಗಿದೆ.",
      "transliteration": "Idu tumba chennagide."
    },
    {
      "english_term": "I need a taxi/auto.",
      "native_term": "ನನಗೆ Auto ಬೇಕು.",
      "transliteration": "Nanage Auto beku."
    },
    {
      "english_term": "See you later.",
      "native_term": "ಆಮೇಲೆ ಸಿಗೋಣ.",
      "transliteration": "Amele sigona."
    },
    {
      "english_term": "What time is it?",
      "native_term": "Time ಎಷ್ಟಾಯ್ತು?",
      "transliteration": "Time eshtaytu?"
    },
    {
      "english_term": "Let's go.",
      "native_term": "ಹೋಗೋಣ ಬನ್ನಿ.",
      "transliteration": "Hogona banni."
    },
    {
      "english_term": "Wait a minute.",
      "native_term": "ಒಂದು ನಿಮಿಷ ಇರಿ.",
      "transliteration": "Ondu nimisha iri."
    },
    {
      "english_term": "Sorry.",
      "native_term": "ಕ್ಷಮಿಸಿ.",
      "transliteration": "Kshamisi."
    }
  ]
}